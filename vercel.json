{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node",
      "config": {
        "runtime": {
          "environment": {
            "VERCEL_ERROR_ON_NOOP": "1"
          }
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}